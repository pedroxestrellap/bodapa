#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/astro@4.16.19_rollup@4.55.1_typescript@5.9.3/node_modules/astro/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/astro@4.16.19_rollup@4.55.1_typescript@5.9.3/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/astro@4.16.19_rollup@4.55.1_typescript@5.9.3/node_modules/astro/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/astro@4.16.19_rollup@4.55.1_typescript@5.9.3/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../astro.js" "$@"
else
  exec node  "$basedir/../../astro.js" "$@"
fi
