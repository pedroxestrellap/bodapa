#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/ulid@3.0.2/node_modules/ulid/dist/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/ulid@3.0.2/node_modules/ulid/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/ulid@3.0.2/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/ulid@3.0.2/node_modules/ulid/dist/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/ulid@3.0.2/node_modules/ulid/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/ulid@3.0.2/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ulid/dist/cli.js" "$@"
else
  exec node  "$basedir/../ulid/dist/cli.js" "$@"
fi
