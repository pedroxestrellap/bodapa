#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/@netlify+zip-it-and-ship-it@14.2.0_rollup@4.55.1/node_modules/@netlify/zip-it-and-ship-it/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/@netlify+zip-it-and-ship-it@14.2.0_rollup@4.55.1/node_modules/@netlify/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/@netlify+zip-it-and-ship-it@14.2.0_rollup@4.55.1/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/@netlify+zip-it-and-ship-it@14.2.0_rollup@4.55.1/node_modules/@netlify/zip-it-and-ship-it/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/@netlify+zip-it-and-ship-it@14.2.0_rollup@4.55.1/node_modules/@netlify/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/@netlify+zip-it-and-ship-it@14.2.0_rollup@4.55.1/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
