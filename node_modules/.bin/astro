#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/astro@5.16.11_@netlify+blobs@10.5.0_@types+node@25.0.9_jiti@1.21.7_rollup@4.55.1_typescript@5.9.3_yaml@2.8.2/node_modules/astro/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/astro@5.16.11_@netlify+blobs@10.5.0_@types+node@25.0.9_jiti@1.21.7_rollup@4.55.1_typescript@5.9.3_yaml@2.8.2/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/astro@5.16.11_@netlify+blobs@10.5.0_@types+node@25.0.9_jiti@1.21.7_rollup@4.55.1_typescript@5.9.3_yaml@2.8.2/node_modules/astro/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/astro@5.16.11_@netlify+blobs@10.5.0_@types+node@25.0.9_jiti@1.21.7_rollup@4.55.1_typescript@5.9.3_yaml@2.8.2/node_modules:/Users/angelofrilop/pedro/personal/boda/web/astro-save-the-date/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
